#evaluate the results of four basic arithmetic

print("BASIC ARITHMITIC")
print()
#Variable values

x = float (input("First number? "))
y = float (input ("Second number? "))
print()

#sums

print()
addition = (x+y)
subtraction = (x-y)
multiplication = (x*y)
division = (x/y)
print()

#print evaluations

print()

print("Addition", addition)

print("Subtraction", subtraction)

print("Multiplication", multiplication)

print("Division", division)

